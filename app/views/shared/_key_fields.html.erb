<td> <%= link_to key.name, edit_project_key_path(@project, key), class: 'keys-links' %></td>
<td>
  <% unless key.url.blank? %>
    <% if /:\/\//.match(key.url) %>
      <%= link_to "#{key.url}", key.url, id: "url_#{key.id}"%>
    <% else %>
      <%= label_tag key.url, key.url, id: "url_#{key.id}" %>
    <% end %>
    <a class='keys-actions' data-clipboard-target='url_<%=key.id%>' id='d_clip_button_url<%=key.id%>' title='Copy to clipboard'><i class="fa fa-clipboard fa-fw"></i></a>
  <% end %>
</td>
<td>
  <% unless key.login.blank? %>
  <label id="login_<%=key.id%>"><%= key.login %></label>
  <a class='keys-actions' data-clipboard-target='login_<%=key.id%>' id='d_clip_button_login<%=key.id%>' title='Copy to clipboard'><i class="fa fa-clipboard fa-fw"></i></a>
  <% end %>
</td>
